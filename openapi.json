    "/data/{symbol}": {
      "get": {
        "description": "Retrieves historical stock or options data for a given symbol.\nCan fetch 'real' data from the database (populated by a separate scraper) or generate 'mock' data.",
        "operationId": "get_financial_data_data__symbol__get",
        "parameters": [
          {
            "in": "path",
            "name": "symbol",
            "required": true,
            "schema": {
              "title": "Symbol",
              "type": "string"
            }
          },
          {
            "description": "Type of data: 'stock' or 'options'",
            "in": "query",
            "name": "data_type",
            "required": true,
            "schema": {
              "description": "Type of data: 'stock' or 'options'",
              "title": "Data Type",
              "type": "string"
            }
          },
          {
            "description": "Data source: 'real' or 'mock'",
            "in": "query",
            "name": "source",
            "required": true,
            "schema": {
              "description": "Data source: 'real' or 'mock'",
              "title": "Source",
              "type": "string"
            }
          },
          {
            "description": "Start date (YYYY-MM-DD). Defaults to 4 months ago for mock, ignored for latest real.",
            "in": "query",
            "name": "start_date",
            "required": false,
            "schema": {
              "anyOf": [
                {
                  "format": "date",
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "description": "Start date (YYYY-MM-DD). Defaults to 4 months ago for mock, ignored for latest real.",
              "title": "Start Date"
            }
          },
          {
            "description": "End date (YYYY-MM-DD). Defaults to today.",
            "in": "query",
            "name": "end_date",
            "required": false,
            "schema": {
              "anyOf": [
                {
                  "format": "date",
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": "2026-01-20",
              "description": "End date (YYYY-MM-DD). Defaults to today.",
              "title": "End Date"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "anyOf": [
                    {
                      "$ref": "#/components/schemas/StockDataResponse"
                    },
                    {
                      "$ref": "#/components/schemas/OptionDataResponse"
                    }
                  ],
                  "title": "Response Get Financial Data Data  Symbol  Get"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "security": [
          {
            "HTTPBearer": []
          }
        ],
        "summary": "Get historical stock or options data for a symbol."
      }
    }
